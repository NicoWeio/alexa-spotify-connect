{
    "intents": [
        {
            "name": "PlayIntent",
            "samples": [
                "play",
                "wiedergabe"
            ]
        },
        {
            "name": "PauseIntent",
            "samples": [
                "pause"
            ]
        },
        {
            "name": "SkipNextIntent",
            "samples": [
                "weiter",
                "{nächster|nächstes} {song|track|lied}",
                "vorwärts"
            ]
        },
        {
            "name": "SkipPreviousIntent",
            "samples": [
                "zurück",
                "{letzter|letztes} {song|track|lied}",
                "rückwärts"
            ]
        },
        {
            "name": "VolumeLevelIntent",
            "slots": {
                "VOLUMELEVEL": "AMAZON.NUMBER"
            },
            "samples": [
                "{ändere die|ändere|} Lautstärke auf {-|VOLUMELEVEL}",
                "lautstärke {-|VOLUMELEVEL}"
            ]
        },
        {
            "name": "GetDevicesIntent",
            "samples": [
                "geräte",
                "liste",
                "suche",
                "finde",
                "{liste|suche|finde|} {alle geräte|alle gerät-nummern} {auf|}"
            ]
        },
        {
            "name": "DevicePlayIntent",
            "slots": {
                "DEVICE": "AMAZON.SearchQuery"
            },
            "samples": [
                "{spiel|spiele|starte|} {musik|spotify|} auf {dem gerät|meinem|} {-|DEVICE}"
            ]
        },
        {
            "name": "DeviceTransferIntent",
            "slots": {
                "DEVICE": "AMAZON.SearchQuery"
            },
            "samples": [
                "{übertrage|ändere} die {musikwiedergabe|wiedergabe} auf {das gerät|mein|} {-|DEVICE}"
            ]
        },
        {
            "name": "GetTrackIntent",
            "samples": [
                "was wird gespielt",
                "was ist {der Song|der Track|das Lied}",
                "{welcher|welches} {song|track|lied} ist das"
            ]
        }
    ]
}
